{
    "compilerOptions": {
        "lib": ["dom", "es5", "es2015.core", "es2015.symbol"],
        "moduleResolution": "node",
        "strictNullChecks": true,
        "target": "es5",
        "outDir": "dist/",
        "declaration": true,
        "noEmitOnError": true
    },
    "include": [
        "src/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}